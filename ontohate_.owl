    <ontohate:inSubontologia xml:lang="pt">Destinatário-vítima</ontohate:inSubontologia>
    <rdfs:comment xml:lang="pt">Pessoa em deslocamento territorial interno ou externo. Pode sofrer estigmatização social. Subclasse individual.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://ontohate.org/ontohate-v2-ufo-rigor/Pessoa_com_caracteristica_protegida"/>
    <ontohate:observacaoV1 xml:lang="pt">Pessoa que se desloca em espaço circunscrito a um território nacional.</ontohate:observacaoV1>
  </rdf:Description>
  <rdf:Description rdf:about="https://ontohate.org/ontohate-v2-ufo-rigor/Contexto_de_normas_culturais">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
