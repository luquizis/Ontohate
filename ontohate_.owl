    <rdfs:comment xml:lang="pt">Classe do domínio ONTOHATE.</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://ontohate.org/ontohate-v2-ufo-rigor/Processo_algoritmico">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="pt">Processo_algoritmico</rdfs:label>
    <rdfs:subClassOf rdf:resource="https://ontohate.org/ontohate-v2-ufo-rigor/Processo"/>
    <rdfs:comment xml:lang="pt">Processo algorítmico executado por sistema técnico (ex.: ranqueamento, recomendação, detecção, classificação).</rdfs:comment>
  </rdf:Description>
  <rdf:Description rdf:about="https://ontohate.org/ontohate-v2-ufo-rigor/UFO_Quality">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label>UFO_Quality</rdfs:label>
    <rdfs:comment xml:lang="pt">Quality (proxy UFO): qualidade dependente de um portador.</rdfs:comment>
    <rdfs:subClassOf rdf:resource="https://ontohate.org/ontohate-v2-ufo-rigor/UFO_Entity"/>
  </rdf:Description>
  <rdf:Description rdf:nodeID="N03fce140a516400cba30c7bb777221e9">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Restriction"/>
    <owl:onProperty rdf:resource="https://ontohate.org/ontohate-v2-ufo-rigor/inereEm"/>
