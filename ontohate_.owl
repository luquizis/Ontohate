    <rdfs:label xml:lang="pt">tinha que ser preto</rdfs:label>
    <ontohate:termoOriginal xml:lang="pt">tinha que ser preto</ontohate:termoOriginal>
  </rdf:Description>
  <rdf:Description rdf:about="https://ontohate.org/ontohate-v2-ufo-rigor/Proposicao">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:label xml:lang="pt">Proposicao</rdfs:label>
